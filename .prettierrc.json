{
  "arrowParens": "always",
  "bracketSameLine": false,
  "bracketSpacing": false,
  "jsonRecursiveSort": true,
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-sort-json"],
  "printWidth": 100,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false
}
